{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "target": "es2020",
    "parser": {
      "syntax": "typescript",
    },
    "transform": {
      "optimizer": {
        "simplify": true
      }
    }
  },
  "module": {
    "type": "es6"
  },
  "sourceMaps": true,
  "exclude": [
    "./lib/**/.*.test.ts$",
  ]
}
