{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint",
    "import",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "camelcase": "off",
    "no-ternary": "off",
    "no-empty": "warn",
    "quotes": [
      "warn",
      "backtick"
    ]
  }
}
